<script>
    function CopyNum(e) {
        switch(e) {
            case 'int':
                let num = '255757259678'
                navigator.clipboard.writeText(num)
                alert('✅ Number copied')
                break;
            case 'tz':
                let num_tz = '0757259678'
                navigator.clipboard.writeText(num_tz)
                alert('✅ Number copied')
                break;
        }
    }
</script>